document.addEventListener("keydown", function(e) {
    if (e.key === "F12" || e.keyCode === 123) {
        e.preventDefault();
    }
});

document.addEventListener("keydown", function(e) {
    if ((e.ctrlKey && e.shiftKey && e.key === "I") || e.keyCode === 123) {
        e.preventDefault();
    }
});
